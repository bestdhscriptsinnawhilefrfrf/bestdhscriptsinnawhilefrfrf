if not game:IsLoaded() then
    game.Loaded:Wait()
end

if getgenv().UniversalProtonLoaded == true then
  Players.LocalPlayer:Kick("Already Injected!")
end

getgenv().UniversalProtonLoaded = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/bestdhscriptsinnawhilefrfrf/bestdhscriptsinnawhilefrfrf/main/proton/main/universal/script"))()
