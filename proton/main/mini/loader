if not game:IsLoaded() then
    game.Loaded:Wait()
end

if getgenv().MiniProtonLoaded == true then
  Players.LocalPlayer:Kick("Already Injected!")
end

getgenv().MiniProtonLoaded = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/bestdhscriptsinnawhilefrfrf/bestdhscriptsinnawhilefrfrf/main/proton/main/mini/script"))()
